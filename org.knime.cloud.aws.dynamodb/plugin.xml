            category-path="/labs/dynamodb"
            deprecated="false"
            factory-class="org.knime.cloud.aws.dynamodb.deletetable.DynamoDBDeleteTableNodeFactory">
      </node>
      <node
            category-path="/labs/dynamodb"
            deprecated="false"
            factory-class="org.knime.cloud.aws.dynamodb.updateitem.DynamoDBUpdateItemNodeFactory">
      </node>
      <node
            category-path="/labs/dynamodb"
            deprecated="false"
            factory-class="org.knime.cloud.aws.dynamodb.putitem.DynamoDBPutItemNodeFactory">
      </node>

   </extension>

   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/ddb.png"
            level-id="dynamodb"
            locked="true"
            name="Amazon DynamoDB"
            path="/labs">
