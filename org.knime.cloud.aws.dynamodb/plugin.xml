            deprecated="false"
            factory-class="org.knime.cloud.aws.dynamodb.batchdelete.DynamoDBBatchDeleteNodeFactory">
      </node>
      <node
            category-path="/labs/dynamodb"
            deprecated="false"
            factory-class="org.knime.cloud.aws.dynamodb.createtable.DynamoDBCreateTableNodeFactory">
